{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "ecmaFeatures": {
      "classes": true
  },
  "settings": {
    "import/extensions": [".js"],
    "import/resolver": {
      "webpack": {
          "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    'no-param-reassign': 0
  },
  "env": {
    "browser": true
  },
}
