{
    "name": "sotdigital/wordplate-empty",
    "description": "Empty Wordplate starter",
    "keywords": [
        "wordplate",
        "wordpress",
        "boilerplate",
        "plate"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "wordplate/plate": "^3.0",
        "wordplate/autoloader": "^1.2",
        "wpackagist-plugin/disable-embeds": "^1.3",
        "wpackagist-plugin/disable-emojis": "^1.7",
        "wpackagist-plugin/normalizer": "^1.0",
        "koodimonni-language/core-sv_se": "^4.8",
        "wpackagist-plugin/better-wp-security": "^6.6",
        "advanced-custom-fields/advanced-custom-fields-pro": "5.6.4",
        "roots/soil": "^3.7",
        "wordplate/polylang": "^3.0",
        "wordplate/framework": "^6.0",
        "wpackagist-plugin/wp-migrate-db": "^1.0",
        "wordplate/mail": "^2.1",
        "ekandreas/bladerunner": "^1.7",
        "wordplate/acf": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "composer",
            "url": "https://wp-languages.github.io"
        },
        {
            "type": "package",
            "package": {
                "name": "advanced-custom-fields/advanced-custom-fields-pro",
                "version": "5.6.4",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9MzQyMTl8dHlwZT1kZXZlbG9wZXJ8ZGF0ZT0yMDE0LTA3LTEwIDA2OjEwOjM3&t=5.6.4"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "update-acf-plugin": [
            "php -r \"file_exists('composer/update-acf-plugin.php') && require 'composer/update-acf-plugin.php';\""
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "installer-paths": {
            "public/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-plugin"
            ],
            "public/themes/{$name}": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "public/wordpress",
        "dropin-paths": {
            "public/languages/": [
                "vendor:koodimonni-language"
            ],
            "public/languages/plugins/": [
                "vendor:koodimonni-plugin-language"
            ],
            "public/languages/themes/": [
                "vendor:koodimonni-theme-language"
            ]
        }
    }
}
